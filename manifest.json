{
  "manifest_version": 3,
  "name": "learnmutiny extension",
  "version": "2.0",
  "description": "learnmutiny beta extension",
  "icons": {
    "16": "assets/bear.png",
    "32": "assets/bear.png",
    "48": "assets/bear.png",
    "128": "assets/bear.png"
  },

  "permissions": [
    "activeTab",
    "tabs",
    "scripting",
    "bookmarks",
    "storage",
    "declarativeNetRequest",
    "geolocation",
    "mdns",
    "proxy"
  ],
  "host_permissions": ["<all_urls>", 
    "https://www.googleapis.com/*"],

  "action": {
    "default_icon": {
      "16": "assets/bear2.png",
      "24": "assets/bear2.png",
      "32": "assets/bear2.png"
    },
    "default_title": "learnmutiny extension",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content.js"]
    }
  ]
}
