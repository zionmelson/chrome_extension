{
  "name": "learnmutiny extension",
  "description": "extension description",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "bear_without_background.png",
    "48": "bear_without_background.png",
    "128": "bear_without_background.png",
    "256": "bear_without_background.png"
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "bear_close_without_background.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["activeTab", "bookmarks", "storage"]
}
